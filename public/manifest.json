{
    "manifest_version": 3,
    "name": "Graphene",
    "version": "0.5.0",
    "description": "View your history as a graph",
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": ["tabs", "storage"],
    "icons": {
      "16": "favicon/extension_icon16.png",
      "32": "favicon/extension_icon32.png",
      "48": "favicon/extension_icon48.png",
      "128": "favicon/extension_icon128.png"
    }
  }